//Accept Number from user & Display Table...

<html>
 <body>
   <script language="javascript">
    n=prompt("Enter Number:");
      for(i=1;i<=10;i++)
	{
 	  document.write("<br>"+n*i);
	}
</script>
</body>
</html>
   
