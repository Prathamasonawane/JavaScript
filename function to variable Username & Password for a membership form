//Write a javascript function to variable Username & Password for a membership form...
//(Username & password is correct then open membership form)

<html>
 <body>
  <form>
	Username:
	   <input type="text" id="t1"><br>
	Password:
	   <input type="password" id="t2"<br>
	   <input type="button" value="login" onclick="check_user()">
   </form>
  </body>
</html>
<script language="javascript">
	function check_user()
	 {
 	   var u=document.getElementById("t1").value;
	   var p=document.getElementById("t2").value;
		if(u=="cdj"&&p=="1234")
	window.location.href="http://www.unipune.ac.in";
		else
	alert("Invalid User or Password...");
	}
</script>
